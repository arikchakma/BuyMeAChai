{
	"name": "buymeachai",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*",
		"contracts/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"clean": "turbo run clean",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "turbo": "latest"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=14.0.0"
	},
	"dependencies": {},
	"packageManager": "yarn@1.22.19"
}
